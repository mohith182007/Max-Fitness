{
  "name": "max-fitness",
  "version": 2,
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "public": true,
  "headers": [
    {
      "source": "/api/submit-form",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml).*)",
      "destination": "/index.html"
    }
  ]
}
